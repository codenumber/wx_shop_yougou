
.goods_list navigator {
  display: flex;
  height: 250rpx;
  height: 100%;
  border-bottom: 2rpx solid #cdcdcd;
}
.goods_image {
  flex: 1.5;
  height: 100%;
}

.goods_desc {
  flex: 3;
  display: flex;
  flex-direction: column;
  justify-content: space-around;
  padding: 10rpx 0 0 20rpx;
}
.goods_name {
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  overflow: hidden;
}
.goods_price {
  color: var(--themeColor);
  font-size: 32rpx;
}
