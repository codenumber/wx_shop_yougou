page {
  height: 100%;
  
}
.cate_container {
  display: flex;
}
.cate_menu {
  height: calc( 100vh - 90rpx );
  flex: 2;
}
.cate_menu view {
  display: flex;
  justify-content: center;
  align-items: center;
  height: 50rpx;
}
.cate_rightcontent {
  flex: 5;
  height: calc( 100vh - 90rpx );
}
.content {
  width: 100%;
  overflow: hidden;
}
.content_title {
  display: flex;
  justify-content: center;
  align-items: center;
  padding: 10rpx;
}

.content navigator {
  float: left;
  width: 33%;
  padding: 10rpx;
}
.content navigator text {
  display: flex;
  justify-content: center;
  align-items: center;
  font-size: 26rpx;
  height: 38rpx;
}
.active {
  border-left: 4rpx solid red;
  color: var(--themeColor);
}